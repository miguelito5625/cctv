<!-- Modals -->


<!-- Modal new entry -->
<div class="modal fade" id="modalNewDevice" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  aria-hidden="true" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Registrar Dispositivo</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">

        <form>

          <div class="form-group">
            <label for="txtCoordinates">Dispositivo</label>
            <div class="row">
              <div class="col-sm-12 col-md-8 col-lg-8" style="margin-top: 15px">
                <select id="selectTypeDevices" class="selectpicker form-control" data-size="5" data-live-search="true">
                </select>
              </div>

              <div class="text-center col-sm-12 col-md-2 col-lg-2" style="margin-top: 15px">
                <button type="button" id="btnOpenModalNewTypeDivece" class="btn btn-primary"
                  style="width: 40px; height: 40px; padding: 6px 6px 6px 6px; font-size: 18px; line-height: 1.33; border-radius: 25px;"><i
                    class="material-icons md-68">add</i></button>
              </div>

              <div class="text-center col-sm-12 col-md-2 col-lg-2" style="margin-top: 15px">
                <button type="button" id="btnRefreshTypeDevices" class="btn btn-primary"
                  style="width: 40px; height: 40px; padding: 6px 6px 6px 6px; font-size: 18px; line-height: 1.33; border-radius: 25px;"><i
                    class="material-icons md-68">refresh</i></button>
              </div>
            </div>
          </div>

          <div class="form-group">
            <label for="txtCoordinates">Marca</label>
            <div class="row">
              <div class="col-sm-12 col-md-8 col-lg-8" style="margin-top: 15px">
                <select id="selectBrands" class="selectpicker form-control" data-size="5" data-live-search="true">
                </select>
              </div>

              <div class="text-center col-sm-12 col-md-2 col-lg-2" style="margin-top: 15px">
                <button type="button" id="btnOpenModalNewBrand" class="btn btn-primary"
                  style="width: 40px; height: 40px; padding: 6px 6px 6px 6px; font-size: 18px; line-height: 1.33; border-radius: 25px;"><i
                    class="material-icons md-68">add</i></button>
              </div>

              <div class="text-center col-sm-12 col-md-2 col-lg-2" style="margin-top: 15px">
                <button type="button" id="btnRefreshBrands" class="btn btn-primary"
                  style="width: 40px; height: 40px; padding: 6px 6px 6px 6px; font-size: 18px; line-height: 1.33; border-radius: 25px;"><i
                    class="material-icons md-68">refresh</i></button>
              </div>

            </div>
          </div>

          <div class="form-group">
            <label for="txtModel">Modelo</label>
            <input type="text" id="txtModel" class="form-control" placeholder="Ingresar el modelo del dispostivo">
          </div>

          <div class="form-group">
            <label for="txtSerialNumber">Numero de serie</label>
            <input type="text" id="txtSerialNumber" class="form-control" placeholder="Ingresar el numero de serie del dispostivo">
          </div>

          <div class="form-group">
            <label for="txtDescription">Descripción</label>
            <textarea type="text" id="txtDescription" class="form-control" placeholder="Opcional"></textarea>
          </div>

          <div class="form-group">
            <label for="txtCoordinates">Gabinete</label>
            <div class="row">
              <div class="col-sm-12 col-md-10 col-lg-10" style="margin-top: 15px">
                <select id="selectCabinets" class="selectpicker form-control" data-size="5" data-live-search="true">
                  <!-- <option data-tokens="ketchup mustard">Hot Dog, Fries and a Soda</option>
                      <option data-tokens="mustard">Burger, Shake and a Smile</option>
                      <option data-tokens="frosting">Sugar, Spice and all things nice</option> -->
                </select>
              </div>
              <div class="text-center col-sm-12 col-md-2 col-lg-2" style="margin-top: 15px">
                <button type="button" id="btnRefreshCabinets" class="btn btn-primary"
                  style="width: 40px; height: 40px; padding: 6px 6px 6px 6px; font-size: 18px; line-height: 1.33; border-radius: 25px;"><i
                    class="material-icons md-68">refresh</i></button>
              </div>
            </div>
          </div>

        </form>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
        <button type="button" id="btnSaveDevice" class="btn btn-primary">Guardar</button>
      </div>
    </div>
  </div>
</div>
<!-- End Modal new entry -->


