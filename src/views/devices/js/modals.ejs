<!-- Modal edit row -->
<div class="modal fade" id="modalEditRow" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  aria-hidden="true" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Editar Gabinete</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">

        <form>
          <div class="form-group">
            <label for="editTxtIdentifier">Identificador</label>
            <input type="text" id="editTxtIdentifier" class="form-control"
              placeholder="Ingresar el Identificador del gabinete">
          </div>
          <div class="form-group">
            <label for="editTxtLocation">Ubicación</label>
            <input type="text" id="editTxtLocation" class="form-control"
              placeholder="Ingresar la Ubicación del gabinete">
          </div>
          <div class="form-group">
            <label for="editTxtCoordinates">Coordenadas</label>
            <input style="background-color: white;" type="text" id="editTxtCoordinates" class="form-control"
              placeholder="Ingresar las Coordenadas del gabinete" readonly>
          </div>

          <input type="hidden" id="editTxtLatitude">
          <input type="hidden" id="editTxtLongitude">


        </form>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
        <button type="button" id="btnUpdate" class="btn btn-primary">Actualizar</button>
      </div>
    </div>
  </div>
</div>
<!-- End Modal edit row -->



<!-- Modal Map -->

<style>
  .pac-container {
    z-index: 1100 !important;
  }
</style>

